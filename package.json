{
  "name": "audiofy",
  "version": "1.0.0",
  "description": "Largest Music Collection",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "start:dev": "NODE_ENV=development nodemon server.js",
    "watch:adminJS": "parcel watch ./public/js/admin.js --out-dir ./public/js --out-file adminBundle.js",
    "build:adminJS": "parcel build ./public/js/admin.js --out-dir ./public/js --out-file adminBundle.js",
    "watch:js": "parcel watch ./public/js/index.js --out-dir ./public/js --out-file bundle.js",
    "build:js": "parcel build ./public/js/index.js --out-dir ./public/js --out-file bundle.js"
  },
  "author": "Aniket Raj",
  "license": "ISC",
  "dependencies": {
    "aws-sdk": "^2.1152.0",
    "axios": "^0.21.4",
    "babel-polyfill": "^6.26.0",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.18.1",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^5.5.1",
    "helmet": "^4.6.0",
    "html-to-text": "^8.2.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.13.14",
    "morgan": "^1.10.0",
    "music-metadata": "^7.12.3",
    "nodemailer": "^6.7.5",
    "pug": "^3.0.2",
    "slugify": "^1.6.5",
    "validator": "^13.7.0",
    "xss-clean": "^0.1.1"
  },
  "devDependencies": {
    "nodemon": "^2.0.16",
    "parcel-bundler": "^1.12.5"
  },
  "engines": {
    "node": ">=10.0.0"
  }
}
